{
  "name": "efureev/laratrust",
  "description": "This package provides a flexible way to add Role-based Permissions to Laravel",
  "keywords": [
    "acl",
    "authorization",
    "laravel",
    "laratrust",
    "permissions",
    "php",
    "rbac",
    "roles",
    "multiusers",
    "teams"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Santiago Garcia",
      "homepage": "http://santigarcor.me"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "kkszymanowski/traitor": "^0.2.5",
    "laravel/framework": "^7.30|^8.30"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^5.18",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Laratrust\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laratrust\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Laratrust\\LaratrustServiceProvider"
      ],
      "aliases": {
        "Laratrust": "Laratrust\\LaratrustFacade"
      }
    }
  }
}
